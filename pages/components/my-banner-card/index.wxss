.container {
    background-color: #DCEBE6;
    height: 600rpx;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.inner-container {
    position: relative;
    display: flex;
    justify-content: center;
    flex-direction: column;
    box-sizing: border-box;
    width: 690rpx;
    height: 520rpx;
    background-color: #ffffff;
    padding-left: 38rpx;
    padding-right: 38rpx;
    box-shadow: 0px 0px 8px 0px rgba(119, 163, 149, 0.2);
    border-radius: 4rpx;
}

.img-container {
    height: 200rpx;
    width: 100%;
}

.img-container image {
    height: 100%;
    width: 100%;
}